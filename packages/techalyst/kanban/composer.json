    "keywords": ["laravel","illuminate","kanban","board","task-manager","kanbanboard"],
    "license": "MIT",
    "authors": [
        {
            "name": "Akram Wahid",
            "email": "akramwahid@live.com",
            "website": "https://www.techalyst.com"
        }
    ],
    "require": {
        "artesaos/seotools": "^0.12.0"
